3570	.d1ad	ca		dex		                dex
3571	.d1ae	ca		dex		                dex                          ;point to X coordinate
3572	.d1af	0a		asl a		                asl a
3573	.d1b0	0a		asl a		                asl a                        ;shift Y outcode into bits 2/3
3574	.d1b1	a0 00		ldy #$00	                ldy #$00                     ;doing Y axis
