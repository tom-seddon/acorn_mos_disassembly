                ldy     #VDUVariables.mocr.src.max
                lda     #VDUVariables.mocr.src.min
                jsr     addRegionDimensionsToVDUVariableCoordinates

                ;
                ldx     #VDUVariables.mocr.src.min
                ldy     #VDUVariables.mocr.dest
                jsr     sortVDUVariableWords

