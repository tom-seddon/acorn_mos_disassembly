                phy
                .if version<500&&version!=350
                jsr getROMInsertedFlagRTCAddressAndMask
                sta starROMSWorkspace.insertedFlagMask
                jsr readRTCByte
                and starROMSWorkspace.insertedFlagMask
                .else
                jsr L873F
                .endif
                bne +
                jsr alwaysPrintFollowingMessage
                .text " unplugged",0
+
                ply              
                jsr OSNEWL       
                dey              
                bpl printROMsLoop
                rts              

invalidROM:
                lda #'?'                     
                jsr OSWRCH                   
                bra printInsertionStatus

